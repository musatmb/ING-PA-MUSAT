#include <iostream>
#include <algorithm>
using namespace std;
int main() {
    int n;
    cout << "Introduceti numarul de elemente: ";
    cin >> n;
    int numere[n];
    cout << "Introduceti " << n << " numere:\n";
    for(int i = 0; i < n; ++i) {
        std::cin >> numere[i];
    }
    sort(numere, numere + n);
    cout << "Numerele in ordine crescatoare sunt:\n";
    for(int i = 0; i < n; ++i) {
        cout << numere[i] << " ";
    }
    cout << std::endl;
    return 0;
}
